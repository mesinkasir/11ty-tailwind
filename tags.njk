---
pagination:
  data: collections
  size: 1
  alias: tag
  filter:
    - all
    - nav
    - post
    - posts
    - tagList
  addAllPagesToCollections: true
layout: layouts/start.njk
eleventyComputed:
  title: Tagged “{{ tag }}”
permalink: /tags/{{ tag | slug }}/
---

{% set postslist = collections[ tag ] %}
{% include "postslist.njk" %}
<div class="sm:text-center">
<p>See <a href="{{ '/tags/' | url }}">all tags</a>.</p>
</div>
