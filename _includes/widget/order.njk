{% for pricing in price %}
 <div class="bg-gray-50 py-5 mt-5">
<h3 class="text-lg font-semibold leading-8 text-indigo-600 font-bold"><a href="{{page.url}}">{{pricing.name}}</a></h3>
<p class="text-lg font-bold">{{pricing.fitur}}</p>
<p class="text-lg font-semibold leading-8 text-indigo-600 font-bold">{{pricing.price}}</p>
 <div class="inline-flex rounded-md shadow">
  <a href="{{pricing.link}}" class="rounded-md bg-indigo-600 px-5 py-3 cols-3 text-base font-medium text-white hover:bg-indigo-700">Order {{pricing.name}}</a>
  </div>
  </div>
{% endfor %}